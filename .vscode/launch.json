  "version": "0.2.0",
  "configurations": [
    {
      "name": "cochi",
      "request": "launch",
      "type": "dart",
      "args": [
        "-d",
        "chrome",
        "--dart-define=FLUTTER_WEB_USE_SKIA=true",
      ]
    },
    {
      "name": "cochi (release mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release",
      "args": [
        "-d",
        "chrome",
        "--release",
        "--no-tree-shake-icons",
        "--dart-define=FLUTTER_WEB_USE_SKIA=true",
      ]
    }
  ]
}